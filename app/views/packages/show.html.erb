<%- model_class = Package -%>
<div class="page-header">
  <h1><%=t '.title', :default => @package.package_type.package_type.titleize %></h1>
</div>

<div class="fieldset">
  <dl>
      <dd><pre class="prettyprint"><%= @package.description %></pre></dd>
  </dl>
</div>
<!-- Search -->
<section id="search" class="search" style="background-color: #f6f6f6;text-align: center;">
    <IFRAME src="<%= "#{ENV['SEARCH_URL']}/pesquisar#{ENV['AFFILIATE_TOKEN']}&#{@package.link}" %>" style='max-width: 1010px;width: 1010px;min-height: 1000px;margin: auto;text-align: center;border: 1px solid #565656;margin-top: 5px;border-radius: 15px;padding:5px;'></IFRAME>
</section>

    <!-- Events -->
    <section id="portfolio" class="bg-primary" style="backgroung-color: #fff;">
        <div class="container">
            <div class="row">
                <div class="col-lg-10 col-lg-offset-1 text-center">
                    <h1 style="text-s">Eventos</h1>
                    <hr class="medium">
                    <div class="row">
                      <% @packages.each do |package| %>
                        <div class="col-md-6">
                            <div class="portfolio-item">
                              <% if package.id == 1 %>
                                <%= link_to(image_tag("/images/seminario_mecanizacao_Fotor.jpg", class: "img-portfolio img-responsive", title: package.description ), package_path(package))%>
                              <% elsif package.id == 2 %>
                                <%= link_to(image_tag("/images/agrishow_Fotor.jpg", class: "img-portfolio img-responsive", title: package.description ), package_path(package))%>
                              <% else %>
                                <h3>Não há Pacote para eventos registrado no momento!</h3>
                              <% end %>
                                <!--
                                <a href="/show?utf8=✓&city_name=Rio+de+Janeiro&state_name=BR&by_latitude=-22.9068467&by_longitude=-43.1728965&by_pack_length=12&room_category=1&by_address=Rio+de+Janeiro%2C+Rio+de+Janeiro+-+RJ%2C+Brasil&by_number_of_people=1&by_checkin_date=06%2F04%2F2016&by_checkin_hour=18&by_checkin_date=06%2F04%2F2016&by_checkin_hour=18&by_checkin_date=06%2F04%2F2016&by_checkin_hour=18">

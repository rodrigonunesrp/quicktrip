    <!-- Events -->
    <section id="portfolio" class="bg-primary" style="backgroung-color: #fff;">
        <div class="container">
            <div class="row">
                <div class="col-lg-10 col-lg-offset-1 text-center">
                    <h1 style="text-s">Eventos</h1>
                    <hr class="medium">
                    <div class="row">
                    <% if @packages.empty? %> 
                      <h3>Não há Pacote para eventos registrado no momento!</h3>
                    <% else %>
                      <% @packages.each do |package| %>
                        <div class="col-md-6">
                          <div class="portfolio-item">
                            <%= link_to(image_tag(package.image, class: "img-portfolio img-responsive", title: package.description ), package_path(package))%>
